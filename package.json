{
  "name": "@kamilbartczak-infer/component-library",
  "version": "0.0.102",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "build": "vite build && npm run build:types",
    "typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit",
    "preview": "vite preview",
    "publish": "npm run build && npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/component-library.umd.js",
  "module": "dist/component-library.es.js",
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.60.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}